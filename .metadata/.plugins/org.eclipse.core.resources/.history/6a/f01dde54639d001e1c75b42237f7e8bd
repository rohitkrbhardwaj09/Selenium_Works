package day2;

public class NumberStrings {

	public static void main(String[] args) {
		
		int a = 123;
		System.out.println(a);
		
		String s = "123";
		System.out.println(s);
		
//		int s = "123"; //Invalid
	
		System.out.println(); //Sysout + ctrl + space
		
		System.out.println("Welcome"+100);
		System.out.println("Welcome"+"100");
		System.out.println("10"+"20");
		System.out.println(10+20);
		
	}

}
