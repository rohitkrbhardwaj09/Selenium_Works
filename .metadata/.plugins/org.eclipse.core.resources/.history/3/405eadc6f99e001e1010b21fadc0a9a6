package Assignments;

import java.util.Arrays;

public class Day6_SortingElements {

	public static void main(String[] args) {
		
		int a[] = {50, 20, 40, 10, 70};
		
		for (int i = 0; i < a.length; i++) {
			if(a[i] < a[i-1]) {
				a[i-1] = a[i];
			}
		}
		
		System.out.println(Arrays.toString(a));

	}

}
